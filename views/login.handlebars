<div class="form-row justify-space-between-md">
  <div>
    <form class="user-form login-form">
      <h1 class="page-title">Login</h1>
      <div>
        <label for="name-login">User Name:</label>
        <input type="text" placeholder="User Name" id="email-login" />
      </div>
      <div>
        <label for="password-login">Password:</label>
        <input type="password" placeholder="Password" id="password-login" />
      </div>
      <div>
        <button class="btn" type="submit">Login</button>
      </div>
    </form>
  </div>
  <div>
    {{>signUp}}
  </div>
</div>